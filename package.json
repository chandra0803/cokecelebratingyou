{
  "name": "g",
  "version": "1.0.0",
  "description": "G6 front end",
  "main": "index.js",
  "scripts": {
    "test": "jest --no-cache",
    "test:watch": "jest --no-cache --watchAll",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "dev": "cross-env NODE_ENV=dev webpack -d --watch --progress --config webpack.dev.config.js & gulp --env=dev",
    "docker": "cross-env NODE_ENV=dev webpack -d --progress --config webpack.dev.config.js && gulp docker --env=docker & cross-env NODE_ENV=dev webpack -d --watch --config webpack.dev.config.js",
    "build": "npm config set registry http://npm-registry.biw-services.com && npm set proxy http://corpuser.corp.biworldwide.com:8080/ && npm set https-proxy http://corpuser.corp.biworldwide.com:8080/ && npm set strict-ssl false && npm install && cross-env NODE_ENV=prod webpack --define process.env.NODE_ENV='\"production\"' --config webpack.prod.config.js --verbose --colors --display-error-details && gulp build --env=build",
    "lint": "eslint -c .eslintrc -f html './src/fe/core/**/*.js' > lintresults.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.biworldwide.com/cpd/g.git"
  },
  "jest": {
    "verbose": true,
    "testRegex": "(/__tests__/).*.js?$",
    "collectCoverageFrom": [
      "src/fe/components/**/*.{js,jsx}",
      "!**/create-store.js",
      "!**/index.js",
      "!**/utils/**",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/fe/components/utils/__tests-utils__/asset.stub.js",
      "\\.(css|scss)$": "<rootDir>/src/fe/components/utils/__tests-utils__/asset.stub.js"
    },
    "testURL": "http://localhost:8001/g6bb8/"
  },
  "author": "",
  "license": "ISC",
