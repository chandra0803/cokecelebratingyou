<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE form-validation PUBLIC
          "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1.3//EN" 
          "http://jakarta.apache.org/commons/dtds/validator_1_1_3.dtd">

<form-validation>
	<formset>

		<!-- Survey Question -->
	    <form name="surveyQuestionForm" >
	      <field property="surveyQuestionText" depends="required">
	        <arg0 key="survey.question.QUESTION" />
	      </field>
	      <field property="surveyQuestionStatus" depends="required">
	        <arg0 key="survey.question.QUESTION_STATUS" />
	      </field>
	    </form>

    </formset>
</form-validation>
